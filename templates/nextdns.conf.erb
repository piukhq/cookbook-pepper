report-client-info false
discovery-dns 
log-queries false
cache-size 0
setup-router true
auto-activate false
config dee4c7
detect-captive-portals false
bogus-priv true
use-hosts true
timeout 5s
listen localhost:53
cache-max-age 0s
max-ttl 0s
hardened-privacy false
max-inflight-requests 256
control /var/run/nextdns.sock
